{
  "name": "epitrello",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && prisma generate",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "nuxt typecheck",
    "auth:schema": "better-auth generate --config ./server/utils/auth.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "prepare": "husky"
  },
  "dependencies": {
    "@better-auth/cli": "1.4.10",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@iconify-json/lucide": "^1.2.83",
    "@iconify-json/simple-icons": "^1.2.65",
    "@nuxt/ui": "^4.3.0",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "better-auth": "1.4.10",
    "nuxt": "^4.2.2",
    "pg": "^8.16.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.12.1",
    "@types/pg": "^8.16.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prisma": "^7.2.0",
    "typescript": "^5.9.3",
    "vue-tsc": "^3.2.2"
  },
  "packageManager": "pnpm@10.28.0",
  "pnpm": {
    "overrides": {
      "@tiptap/core": "3.15.3",
      "@tiptap/pm": "3.15.3",
      "magicast": "0.5.1"
    }
  }
}
